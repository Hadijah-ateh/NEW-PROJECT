pipeline{

    agent any

    stages{

        stage('Continuos Download') {

            steps{
                git branch: 'main', url: 'https://github.com/Hadijah-ateh/NEW-PROJECT.git'
            }
        }  
        stage('Unit Test'){

            steps{
                sh'mvn test'
            }
        }  
        stage('Integration Test'){

            steps{
                sh'mvn verify -DskipUnitTests'
            }
        }  
        stage('Continous Build'){

            steps{
                sh'mvn clean install'
            }
        }   
    }
}